#define PI  3.141569
#define TAU 6.283185

//-----------------------------------------------------------------------------|
// GLOBAL VARIABLES
//-----------------------------------------------------------------------------|

int    INT_PI     = 3;
int    INT_TAU    = 6;
float  FLOAT_PI   = 3.14159265;
float  FLOAT_TAU  = 6.28318530;
double DOUBLE_PI  = 3.141592653589793238;
double DOUBLE_TAU = 6.283185307179586476;

//-----------------------------------------------------------------------------|
// NAMESPACES
